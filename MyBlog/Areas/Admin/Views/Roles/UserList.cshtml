@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>

<div class="container py-5">
    <a asp-area="Admin" asp-controller="Home" asp-action="Index">Вернуться на панель администратора</a>
    <div class="card mb-4" id="tables">
        <div class="card-header">Список пользователей</div>
        <div class="card-body">
            <table class="table table-hover">
                <tbody>
                    @foreach (var user in Model)
                    {
                        <tr>
                            <td>@user.Email</td>
                            <td>
                                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>